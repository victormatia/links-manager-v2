{
  "name": "links-manager-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run db:generate && next dev",
    "build": "next build ",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:reset": "dotenv -e .env.local -- npx prisma db push --force-reset"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.2",
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.39.0",
    "@vercel/postgres": "^0.8.0",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.381.0",
    "moment": "^2.30.1",
    "next": "^14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "postcss": "^8",
    "prisma": "^5.14.0",
    "tailwindcss": "^3",
    "typescript": "^5.4.5"
  }
}
